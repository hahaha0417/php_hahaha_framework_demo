{
    "name": "hahaha/hahaha",
    "description": "The PHP Hahaha Framework.",
    "keywords": ["framework", "hahaha", "hahahalib", "windows"],
    "license": "MIT",
    "type": "project",
    "require": {
        "botman/botman": "^2.5",
        "botman/driver-facebook": "^1.10",
        "doctrine/orm": "^2.6.2",
        "linecorp/line-bot-sdk": "^3.14",
        "malkusch/lock": "^2.1",
        "monolog/monolog": "^2.0",
        "signe/redlock-php": "^1.1",
        "symfony/yaml": "2.*"
    },
    "require-dev": {
    },
    "autoload": {
        "classmap": [
            "framework",
			"libraries",
			"app",
            "bootstrap",
            "config",
            "table",
            "flow",
            "resources"

        ],
		"psr-0": {"": "src/"}
    },
    "autoload-dev": {
    },
    "extra": {
        "hahaha": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
